{
  "name": "voyager",
  "version": "2.0.0-alpha.0",
  "description": "",
  "author": {
    "name": "UW Interactive Data Lab",
    "url": "http://idl.cs.washington.edu"
  },
  "collaborators": [
    "Kanit Wongsuphasawat <kanitw@gmail.com> (http://kanitw.yellowpigz.com)",
    "Dominik Moritz <domoritz@cs.washington.edu> (http://domoritz.de)",
    "Jeffrey Heer <jheer@uw.edu> (http://jheer.org)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vega/voyager.git"
  },
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "webpack -d",
    "clean": "find -E dist  -regex '.*\\.(js|js.map|d.ts)' -delete",
    "lint": "tslint -c tslint.json 'src/**/*.ts' --exclude '**/*.d.ts'",
    "test": "jest --no-cache --coverage"
  },
  "dependencies": {
    "@types/jest": "^18.1.1",
    "compassql": "^0.8.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux-logger": "^2.8.1",
    "tslib": "^1.5.0",
    "vega-lite": "2.0.0-alpha.1"
  },
  "devDependencies": {
    "@types/react": "^15.0.8",
    "@types/react-dom": "^0.14.23",
    "autoprefixer": "6.4.1",
    "codecov": "^1.0.1",
    "jest": "~18",
    "source-map-loader": "^0.1.6",
    "ts-jest": "~18",
    "ts-loader": "^2.0.0",
    "tslint": "~4.3.1",
    "tslint-react": "^2.3.0",
    "typescript": "^2.1.5",
    "webpack": "^1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testPathIgnorePatterns": ["<rootDir>/dist/", "<rootDir>/node_modules/"],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
